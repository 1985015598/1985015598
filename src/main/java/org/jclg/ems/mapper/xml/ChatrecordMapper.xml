<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jclg.ems.mapper.ChatrecordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.jclg.ems.bean.Chatrecord">
        <id column="id" property="id" />
        <result column="content" property="content" />
        <result column="sendsta" property="sendsta" />
        <result column="receive" property="receive" />
        <result column="mestype" property="mestype" />
        <result column="sendid" property="sendid" />
        <result column="overid" property="overid" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, content, sendsta, receive, mestype, sendid, overid, remark
    </sql>

</mapper>
